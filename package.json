{
  "name": "campfire-allowed-near-me",
  "version": "1.0.0",
  "description": "Closest legal campfire finder for NSW state forests",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "tsx scripts/dev.ts",
    "dev:web": "vite --config apps/web/vite.config.ts",
    "build": "npm run build:web",
    "build:web": "vite build --config apps/web/vite.config.ts",
    "typecheck": "tsc -p apps/api/tsconfig.json --noEmit && tsc -p apps/web/tsconfig.json --noEmit",
    "test": "npm run typecheck && npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:integration:nominatim-live": "NOMINATIM_LIVE_TEST=1 vitest run tests/integration/nominatim-live.test.ts",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "warm:coordinates": "tsx apps/api/src/scripts/warm-coordinate-cache.ts",
    "cache:reset": "tsx apps/api/src/scripts/reset-caches.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mantine/core": "^8.3.15",
    "@mantine/hooks": "^8.3.15",
    "@tabler/icons-react": "^3.37.1",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-virtual": "^3.13.18",
    "cheerio": "^1.2.0",
    "dotenv": "^17.3.1",
    "leaflet": "^1.9.4",
    "p-limit": "^7.3.0",
    "playwright": "^1.58.2",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-leaflet": "^5.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "jsdom": "^28.1.0",
    "msw": "^2.12.10",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
